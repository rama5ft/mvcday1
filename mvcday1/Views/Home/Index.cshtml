@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron" style="background-color:coral">
    <center>
        <h1 style="color:black">Movie Store</h1>
    </center>
</div>
<div>
    @*@TempData["CountryName"]*@
    <text>Country NAme:</text>@ViewBag.CountryName
</div>

<div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="~/images/event-management-background-12.jpg " width="350" height="350" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
            <img src="~/images/photo-1492684223066-81342ee5ff30.jpg" width="350" height="350" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
            <img src="~/images/rose-event-management-kovaipudur-coimbatore-event-organisers-3znkxp6.jpg" width="350" height="350" class="d-block w-100" alt="...">
        </div>
    </div>

    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

